{
  "alp": {
    "head-tag": {
      "title": "The Ad Landing Page",
      "canonical-path": "templates/alp.amp",
      "extensions": [
        "amp-carousel",
        "amp-sidebar",
        "amp-form",
        "amp-call-tracking"
      ],
      "font-stylesheets": [
      ],
      "css-path": "templates/alp/page.css"
    },
    "navbar": {
      "name": "The Ad Landing Page",
      "sidebar-id": "header-sidebar",
      "sidebaronly": true,
      "logo": {
         "name": "Company Name"
      },
      "faq": {
        "links": [
          {
            "link": {"url": "#", "text": "About Us"}
          },
          {
            "link": {"url": "#", "text": "Services"}
          }
        ],
        "calltracking": {
          "config": "https://example.com/calltracking.json",
          "text": "Call now: (800)555-5555",
          "href": "tel:8005555555"
        }
      }
    },
    "form": {
      "title": "Contact us for a free consultation",
      "method": "GET",
      "action": "/",
      "inputs": [
        {
          "input": {
            "type": "Full name",
            "value": "",
            "placeholder": "Full name",
            "id": "fullnameid",
            "name": "fullnameid"
          }
        },
        {
          "input": {
            "type": "State",
            "value": "",
            "placeholder": "State",
            "id": "stateid",
            "name": "stateid"
          }
        },
        {
          "input": {
            "type": "phone",
            "value": "",
            "placeholder": "Phone",
            "id": "phoneid",
            "name": "phoneid"
          }
        },
        {
          "input": {
            "type": "email",
            "value": "",
            "placeholder": "Email",
            "id": "emailid",
            "name": "emailid"
          }
        },
        {
          "submit": {
            "id": "submit",
            "name": "submit",
            "value": "Submit",
            "classes": "ampstart-btn-secondary"
          }
        }
      ]
    },
    "calltracking": {
      "config": "https://example.com/calltracking.json",
      "text": "Call now: (800)555-5555",
      "href": "tel:8005555555"
    },
    "footer": {
      "social-follow": false,
      "faq-links": {
        "links": [
          {"text": "About", "url": "#"},
          {"text": "Services", "url": "#"}
        ]
      },
      "copyright": {
        "text": "Â©2017 Company Name"
      }
    }
  }
}
