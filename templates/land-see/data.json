      {
        "link": {"url": "#", "nav-item-classes": "land-see-sidebar-nav-item", "text": "Shop"}
      },
      {
        "link": {"url": "#", "nav-item-classes": "land-see-sidebar-nav-item", "text": "About"}
      },
      {
        "link": {"url": "#", "nav-item-classes": "land-see-sidebar-nav-item", "text": "Contact"}
      }
