{
  "land-see": {
    "head-tag": {
      "title": "Land + See",
      "canonical-path": "templates/land-see/land-see.amp",
      "extensions": [
        "amp-carousel",
        "amp-sidebar",
        "amp-accordion",
        "amp-instagram",
        "amp-list",
        "amp-form",
        "amp-anim",
        "amp-animation",
        "amp-position-observer",
        "amp-bind"
      ],
      "templates": [
        "amp-mustache"
      ],
      "css-path": "templates/land-see/page.css",
