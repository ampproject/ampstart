{
  "lune": {
    "head-tag": {
      "title": "Lune",
      "canonical-path": "templates/lune.amp",
      "extensions": [
        "amp-sidebar"
      ],
      "css-path": "templates/lune/page.css"
    }
  },

  "landing-sections": [
    {
      "classes": "z1 relative lune-theme-light",
      "full_title": "Lune blanc",
      "product_image_srcset": "../../img/lune/landing-hero-blanc@1x.png, ../../img/lune/landing-hero-blanc@2x.png 2x",
      "product_name_split": ["b", "l", "a", "n", "c"],
      "cta": {
        "text": "Discover blanc",
        "href": "product-blanc.amp.html",
        "classes": "lune-anim-move-up z2"
      },
      "scroll-indicator": {
        "classes": "relative"
      }
    },
    {
      "classes": "z0 relative top-0 left-0 right-0 bottom-0 lune-theme-dark",
      "full_title": "Lune noir",
      "product_image_srcset": "../../img/lune/landing-hero-noir@1x.png, ../../img/lune/landing-hero-noir@2x.png 2x",
      "product_name_split": ["n", "o", "i", "r"],
      "cta": {
        "text": "Discover noir",
        "href": "product-noir.amp.html",
        "classes": "lune-anim-move-up z2"
      },
      "scroll-indicator": {
        "classes": "relative"
      }
    }
  ]
}
