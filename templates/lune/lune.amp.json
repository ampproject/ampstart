{
  "lune": {
    "head-tag": {
      "title": "Lune",
      "canonical-path": "templates/lune.amp",
      "extensions": [
        "amp-position-observer",
        "amp-animation"
      ],
      "css-path": "templates/lune/page.css"
    }
  },

  "landing-sections": [
    {
      "classes": "z2 lune-theme-light",
      "full_title": "Lune blanc",
      "product_360_image": {
        "alt": "Lune Blanc",
        "webp_layers": "../../img/lune/360/blanc",
        "fallback_layers": "../../img/lune/360/blanc",
        "total_layers": 3,
        "width": 1,
        "height": 1.37,
        "placeholder_classes": "lune-theme-light"
      },
      "product_name_split": ["b", "l", "a", "n", "c"],
      "cta": {
        "text": "Discover blanc",
        "href": "product-blanc.amp.html",
        "classes": "lune-anim-move-up z2 lune-theme-light lune-transparent"
      },
      "scroll-indicator": {
        "classes": "relative"
      }
    },
    {
      "classes": "z1 lune-theme-dark",
      "full_title": "Lune noir",
      "product_360_image": {
        "alt": "Lune Noir",
        "webp_layers": "../../img/lune/360/noir",
        "fallback_layers": "../../img/lune/360/noir",
        "total_layers": 3,
        "width": 1,
        "height": 1.37,
        "placeholder_classes": "lune-theme-dark"
      },
      "product_name_split": ["n", "o", "i", "r"],
      "cta": {
        "text": "Discover noir",
        "href": "product-noir.amp.html",
        "external": false,
        "classes": "lune-anim-move-up z2 lune-theme-dark lune-transparent"
      },
      "scroll-indicator": {
        "classes": "relative"
      }
    }
  ]
}
