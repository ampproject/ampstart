{
  "header": {
    "head-tag": {
      "canonical-path": "templates/e-commerce/cart.amp",
      "extensions": [
        "amp-sidebar",
        "amp-carousel"
      ]
    },
    "navbar": {
      "trigger-layout-classes": "absolute top-0",
      "trigger-spacing-classes": "pr0 mr2 mt2"
    }
  },
  "related-products": {
    "responsive-display-classes": "xs-hide sm-hide"
  },
  "cart": {
    "showCartActions": true,
    "item": {
      "spacing-classes": "mx2 mb2",
      "responsive-spacing-classes": "md-mx4 md-mt3 md-pb3"
    },
    "header": {
      "classes": "h3",
      "spacing-classes": "mb2 mx2",
      "responsive-spacing-classes": "md-mx4"
    }
  }
}
