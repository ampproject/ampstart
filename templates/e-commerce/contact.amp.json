{
  "header": {
    "head-tag": {
      "canonical-path": "templates/e-commerce/landing.amp",
      "extensions": [
        "amp-sidebar"
      ]
    },
    "navbar": {
      "trigger-layout-classes": "absolute top-0",
      "trigger-spacing-classes": "pr0 mr2 mt2"
    }
  },
  "contact": [
    {
      "input": {
        "type": "text",
        "placeholder": "Full name",
        "name": "name",
        "id": "name"
      }
    },
    {
      "input": {
        "type": "text",
        "placeholder": "E-Mail address",
        "name": "email",
        "id": "email"
      }
    },
    {
      "textarea": {
        "name": "mesage",
        "id": "message",
        "placeholder": "Type your message below",
        "rows": 2,
        "classes": "mt3 mb5"
      }
    }
  ]
}
