{
  "header": {
    "head-tag": {
      "canonical-path": "templates/e-commerce/checkout.amp",
      "extensions": [
        "amp-sidebar",
        "amp-bind"
      ]
    }
  },
  "checkout": {
    "address": [
      {
        "input": {
          "type": "text",
          "placeholder": "First name",
          "name": "first-name",
          "id": "first-name"
        }
      },
      {
        "input": {
          "type": "text",
          "placeholder": "Last name",
          "name": "last-name",
          "id": "last-name"
        }
      },
      {
        "input": {
          "type": "text",
          "placeholder": "Address line 1",
          "name": "address-1",
          "id": "address-1"
        }
      },
      {
        "input": {
          "type": "text",
          "placeholder": "Address line 2",
          "name": "address-2",
          "id": "address-2"
        }
      },
      {
        "input": {
          "type": "text",
          "placeholder": "City/Region",
          "name": "city",
          "id": "city"
        }
      },
      {
        "input": {
          "type": "text",
          "placeholder": "Postcode",
          "name": "postcode",
          "id": "postcode"
        }
      },
      {
        "input": {
          "type": "email",
          "placeholder": "E-Mail Address",
          "name": "email",
          "id": "email"
        }
      }
    ],
    "delivery-method": {
      "radio": [
        {
          "name": "delivery-method",
          "id": "standard",
          "label": "Standard (arrives in 2-3 days) - FREE",
          "checked": "checked",
          "on": "change:AMP.setState({cart: {total: 723, shipping: 0}})"
        },
        {
          "name": "delivery-method",
          "id": "express",
          "label": "Express (arrives next day) - Â£30",
          "on": "change:AMP.setState({cart: {total: 753, shipping: 30}})"
        }
      ]
    }
  },
  "cart": {
    "item": {
      "spacing-classes": "mx2 mb2",
      "responsive-spacing-classes": "md-mx4 md-mt3 md-pb3"
    },
    "header": {
      "classes": "h5",
      "responsive-layout-classes": "xs-hide sm-hide"
    },
    "dynamic": true
  }
}
